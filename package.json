{
  "name": "vue2-base",
  "version": "0.0.1",
  "description": "vue2 & element-ui 基础管理平台",
  "scripts": {
    "start": "webpack serve --mode=development",
    "build": "webpack --mode=production",
    "fix": "pnpm lint && pnpm prettier",
    "lint": "eslint src/**/*.{vue,js,jsx} --fix",
    "prettier": "prettier src/**/*.{vue,js,jsx} --write"
  },
  "keywords": [
    "vue2",
    "element-ui"
  ],
  "author": "948533798@qq.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.21.5",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-vue": "^9.11.0",
    "ghooks": "^2.0.4",
    "html-webpack-plugin": "^5.5.1",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "lint-staged": "^13.2.2",
    "mini-css-extract-plugin": "^2.7.5",
    "prettier": "^2.8.8",
    "style-loader": "^3.3.2",
    "vue-eslint-parser": "^9.2.0",
    "vue-loader": "^15.10.1",
    "vue-template-compiler": "~2.6.14",
    "webpack": "^5.82.0",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^4.13.3"
  },
  "dependencies": {
    "vue": "~2.6.14"
  },
  "lint-staged": {
    "*.{vue,js,jsx}": [
      "pnpm fix"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "npx lint-staged --quiet --concurrent false"
    }
  }
